{
    "name": "instabot",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "dev": "remix build && run-p \"dev:*\"",
        "dev:node": "cross-env NODE_ENV=development nodemon",
        "dev:remix": "remix watch",
        "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
        "build": "remix build && run-p \"build:*\"",
        "build:node": "node server.build.js",
        "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
        "db:migrate": "prisma migrate dev --name",
        "db:wipe": "prisma migrate reset dev",
        "start": "cross-env NODE_ENV=production node ./dist/index.js"
    },
    "devDependencies": {
        "@babel/core": "^7.18.13",
        "@remix-run/dev": "^1.7.0",
        "@remix-run/eslint-config": "^1.7.0",
        "@types/bluebird": "^3.5.36",
        "@types/compression": "^1.7.2",
        "@types/express": "^4.17.13",
        "@types/fluent-ffmpeg": "^2.1.20",
        "@types/jsonwebtoken": "^8.5.8",
        "@types/morgan": "^1.9.3",
        "@types/react": "^18.0.17",
        "@types/react-dom": "^18.0.6",
        "autoprefixer": "^10.4.8",
        "babel-loader": "^8.2.5",
        "bluebird": "^3.7.2",
        "cross-env": "^7.0.3",
        "esbuild": "^0.15.5",
        "eslint": "^8.22.0",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.16",
        "prisma": "^4.2.1",
        "tailwindcss": "^3.1.8"
    },
    "dependencies": {
        "@headlessui/react": "^1.6.6",
        "@prisma/client": "^4.2.1",
        "@remix-run/express": "^1.7.0",
        "@remix-run/node": "^1.7.0",
        "@remix-run/react": "^1.7.0",
        "compression": "^1.7.4",
        "cookie-parser": "^1.4.6",
        "dotenv": "^16.0.1",
        "express": "^4.18.1",
        "fluent-ffmpeg": "^2.1.2",
        "instagram-private-api": "^1.45.3",
        "jsonwebtoken": "^8.5.1",
        "morgan": "^1.10.0",
        "node-fetch": "^3.2.10",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "tiny-invariant": "^1.2.0"
    }
}
